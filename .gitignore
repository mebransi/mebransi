p, td, tr, div {
font-style: normal;
font-weight: normal;
font-variant: normal;
cursor: hand;
font-size: 11px;
font-family: Verdana; 
color: #a8a8a8;
}

body {body-align: right; width: 960px;   margin: auto; background: #2b2b2b; padding: 15px 0px 10px 0px;}
html { 	background: #8d8d8d url(''); -moz-border-radius: 10px; }



/*--- links ---*/

a, a:link, a:visited, a:active{
color: a8a8a8; 
font-family: georgia;
font-size: 10px;
text-transform: uppercase; 
TEXT-DECORATION: none;
letter-spacing: 0px;
cursor: hand; padding: 1px;
background: none.
font-weight: normal;
font-style: normal;
 }

a:hover {color: a8a8a8;
letter-spacing: 0px;
text-transform: uppercase; 
TEXT-DECORATION: none;
cursor: hand;
background: none;
font-family: georgia;
font-size: 10px;
font-weight: normal;
font-style: normal;
 }

/*--- coments ---*/
input, textarea, select {
    font-family: verdana;
    font-size: 11px; 
    text-align: justify;
    color: #959595;
    border: none;
    -moz-border-radius: 0px;
    line-height: 11px;
    padding: 5px;
    cursor: size;
    background-color: #000000;}

strong {   
      color: #a8a8a8; 
      font-style: none; 
      font-weight: normal; 
      text-decoration: none;
      font: 11px verdana;
      letter-spacing: 0px;
      text-transform: none; }

/*----------- BARRA -----------*/
#barra {
   margin-right: auto; padding:0;
   height: 45px;text-transform: none;
   background: #a7a7a7;
   border: none;
   text-align: center;}


#barra .tubo {
   background: #a7a7a7; text-transform: none;
   padding:0px;
   height: 5px; width: 100%;
   border-top: 0px solid #none;
   border-bottom: 0px solid #none;
   border-right: 0px solid #none;
   border-left: 0Px solid #none;}


#barra .fvbarraf {
   margin: 0;
   padding:2px;
   width: 96px;text-transform: uppercase;
   height: 15px;
   background-color: #a7a7a7;
   border-top: none;
   border-bottom: none;
   border-left: none;
   border-right: none;
   font:  10px georgia; 
   color:#a7a7a7; 
   }


#barra .fvbarra {
   font: 11px georgia;text-transform: none;
   color: #bfbfbf; border: none;}


#barra .fvbarra a:link, #barra .fvbarra a:visited , #barra .fvbarra a:active  {
        color: #c6c6c6;
        font-family: georgia;
        font-size: 11px; cursor: hand;
        line-height: 45px;text-transform: uppercase;
        text-align:center;
        padding: 7px; letter-spacing: 0px;
        border-top: 0px solid #c6c6c6;
        border-left: 0px solid #c6c6c6;
        border-right: 0px solid #c6c6c6;
        border-bottom: 0px solid #c6c6c6;
        -moz-border-radius: 0px 0px 0px 0px;
        background-color: #aaaaaa;}

#barra .fvbarra a:hover {color: #c6c6c6;
	cursor: hand;
        font-family: georgia;
        font-weight: normal;
        font-style: normal;
        text-decoration: none; text-transform: uppercase;
        font-size: 11px;
        line-height: 45px;
        text-align:left;
        padding: 7px; letter-spacing: 0px;
        border-top: 0px solid #dddddd;
        border-left: 0px solid #dddddd;
        border-right: 0px solid #dddddd;
        border-bottom: 0px solid #9b9b9b;
        -moz-border-radius: 0px 0px 0px 0px;
        background-color: #acbed0;}



#barra .barraimg { 
-moz-opacity:1.0; 
   opacity: 0.00;}



